    <ui:VisualElement name="StatusBar" class="defaultContainer" style="position: absolute; left: 32px; top: 0.5px; margin-left: 5px; margin-right: 5px; margin-top: 5px; margin-bottom: 5px; width: 31%; height: 35px; flex-direction: row; align-items: center; border-top-left-radius: 0; border-bottom-left-radius: 0;">
